{
  "$schema": "http://json-schema.org/draft-07/schema#",
  "type": "object",
  "properties": {
    "title": {
      "type": "string"
    },
    "objective": {
      "type": "string"
    },
    "typeOfUser": {
      "type": "string"
    },
    "learningMode": {
      "type": "string",
      "enum": ["self-paced", "instructor-led"]
    },
    "competencies": {
      "type": "object"
    },
    "referenceLink": {
      "type": "string"
    },
    "requestType": {
      "type": "string",
      "enum": ["Broadcast", "Single"]
    },
    "preferredProvider": {
      "type": "array",
      "items": {
        "type": "object"
      }
    },
    "status": {
      "type": "string"
    },
    "source": {
      "type": "string"
    }
  },
  "required": [
    "title",
    "objective",
    "requestType"
  ]
}