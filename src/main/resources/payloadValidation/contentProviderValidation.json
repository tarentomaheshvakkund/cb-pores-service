{
  "$schema": "http://json-schema.org/draft-07/schema#",
  "type": "object",
  "properties": {
    "id": {
      "type": "string"
    },
    "contentPartnerName": {
      "minLength": 3,
      "maxLength": 70,
      "pattern": "^[a-zA-Z][a-zA-Z0-9\\.\\-_\\$/:\\[\\]'!\\s]*$",
      "type": "string"
    },
    "description": {
      "minLength": 3,
      "maxLength": 500,
      "type": "string",
      "pattern": "^[a-zA-Z][a-zA-Z0-9\\.\\-_\\$/:\\[\\]'!\\s]*$"
    },
    "isActive": {
      "type": "boolean"
    },
    "link": {
      "type": "string",
      "format": "uri"
    },
    "websiteUrl": {
      "type": "string",
      "format": "uri"
    },
    "documentUrl": {
      "type": "string",
      "format": "uri"
    },
    "providerTips": {
      "type": "array",
      "items": {
        "type": "string"
      }
    },
    "createdOn": {
      "type": "string"
    },
    "updatedOn": {
      "type": "string"
    },
    "trasformContentJson": {
      "type": "array"
    },
    "transformProgressJson": {
      "type": "array"
    },
    "trasformCertificateJson": {
      "type": "object"
    },
    "certificateTemplateUrl": {
      "type": "string"
    },
    "partnerCode": {
      "minLength": 3,
      "maxLength": 6,
      "pattern": "^[A-Z][A-Z0-9]*$",
      "type": "string"
    },
    "isAuthenticate": {
      "type": "boolean"
    },
    "totalCoursesCount": {
      "type": "number"
    },
    "draftCoursesCount": {
      "type": "number"
    },
    "liveCoursesCount": {
      "type": "number"
    },
    "searchTags": {
      "type": "array",
      "items": {
        "type": "string"
      }
    },
    "serviceRegistryDetails": {
      "type": "object"
    },
    "documentUploadedDate": {
      "type": "string"
    }
  },
  "required": [
    "contentPartnerName",
    "websiteUrl",
    "link",
    "description",
    "partnerCode",
    "totalCoursesCount",
    "draftCoursesCount",
    "liveCoursesCount"
  ],
  "additionalProperties": false
}
